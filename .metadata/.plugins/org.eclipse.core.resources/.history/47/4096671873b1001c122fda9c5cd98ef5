package com.samples.oop7.abstractclass;

public class Bird {
	public Bird(String name) {
		super(name);
	}

	@Override
	public void eat() {
		System.out.println(getName() + " is pecking");
	}

	@Override
	public void breathe() {
		System.out.println(getName() + " is breathing");
	}
	
	public abstract void fly();

}
