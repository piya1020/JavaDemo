package com.samples.javademos.oop4;

public class PrinterTest {
	public static void main(String[] args) {
		Printer p=new Printer(50,true);
		System.out.println(" Intial page count= " + printer.getPagesPrinted());
		int pagesPrinted = p.printpages(4);
		System.out.println(" pages printed was " + pagesPrinted + " new total print count for Printer " + printer);
	}

}
