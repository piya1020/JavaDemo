package com.teksystems.myproject;

public class DemoSwitch {
	public static void main(String[] args) {
		
		int switchval=3;
		
		switch(switchval)
		{
			case 1:
				System.out.println("case 1");
				break;
			case 2:
				System.out.println("case 2");
				break;
			case 3:
				System.out.println("case 3");
				break;
			default:
				System.out.println("Other Value");
				
		}
			
		
	}

}
